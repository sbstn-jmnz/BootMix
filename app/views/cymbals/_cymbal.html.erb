<tr>
	<td><%= cymbal.position %></td>
	<td><%= cymbal.CymbalType %></td>
	<td><%= cymbal.family %></td>
	<td><%= cymbal.inches %></td>
	<td><%= cymbal.name %></td>
	<td><image src="<%= cymbal.image %>" style="width:75px" alt=""</td>
	<td><audio controls>
		<source src="<%= asset_path cymbal.sound%>" type="audio/ogg">
		<source src="<%= asset_path cymbal.sound%>" type="audio/wav">
		<source src="<%= asset_path cymbal.sound%>" type="audio/mpeg">
				Your browser does not support the audio element.
		</audio>
	</td>
	<td><%= link_to 'Show', cymbal %></td>
	<td><%= link_to 'Edit', edit_cymbal_path(cymbal) %></td>
	<td><%= link_to 'Destroy', cymbal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
